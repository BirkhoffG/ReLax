{
    "data_configs": {
        "data_dir": "assets/data/extra/s_german_credit.csv",
        "batch_size": 128,
        "continous_cols": [
            "duration",
            "credit_amount",
            "installment_rate",
            "residence",
            "age",
            "credits",
            "num_people_liable"
        ],
        "discret_cols": [
            "status",
            "history",
            "purpose",
            "savings",
            "employment",
            "sex",
            "other_debtors",
            "property",
            "other_plan",
            "housing",
            "job",
            "telephone",
            "foreign_worker"
        ]
    },
    "mlp_configs": {
        "sizes": [50, 10, 50],
        "dropout_rate": 0.3,
        "lr": 0.003
    },
    "cfnet_configs": {
        "enc_sizes": [50,10],
        "dec_sizes": [10],
        "exp_sizes": [50, 50],
